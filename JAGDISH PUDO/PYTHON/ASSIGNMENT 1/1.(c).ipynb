    "def diamond(n=3):\n",
    "    \n",
    "    for i in range(1, n + 1):\n",
    "        print(\" \" * (n - i) + \"*\" * (2 * i - 1))\n",
    "    \n",
    "    for i in range(n - 1, 0, -1):\n",
    "        print(\" \" * (n - i) + \"*\" * (2 * i - 1))\n",
    "\n",
    "diamond()"
